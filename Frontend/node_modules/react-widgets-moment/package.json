  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/react-widgets/issues"
  },
  "homepage": "https://github.com/jquense/react-widgets#readme",
  "peerDependencies": {
    "moment": ">=2.2.0",
    "react-widgets": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "cross-env": "^5.2.0",
    "moment": "^2.21.0",
    "react-widgets": "^4.6.1",
    "rimraf": "^2.6.2"
  }
}
